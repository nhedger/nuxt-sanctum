{
	"name": "@hedger/nuxt-sanctum",
	"publishConfig": {
		"access": "public",
		"provenance": true
	},
	"version": "0.0.2",
	"description": "Nuxt module for Laravel Sanctum",
	"repository": "nhedger/nuxt-sanctum",
	"license": "MIT",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/types.d.ts",
			"import": "./dist/module.mjs",
			"require": "./dist/module.cjs"
		}
	},
	"main": "./dist/module.cjs",
	"types": "./dist/types.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "nuxi prepare playgrounds/nuxt && nuxt-module-build",
		"dev": "run-p --print-label dev:nuxt dev:laravel",
		"dev:nuxt": "nuxi dev playgrounds/nuxt",
		"dev:build": "nuxi build playgrounds/nuxt",
		"dev:prepare": "nuxt-module-build --stub && nuxt-module-build prepare && nuxi prepare playgrounds/nuxt",
		"dev:laravel": "cd playgrounds/laravel && php artisan serve --host=\"[::1]:8000\"",
		"lint": "eslint .",
		"test": "vitest run",
		"test:watch": "vitest watch"
	},
	"dependencies": {
		"@nuxt/kit": "^3.7.3",
		"defu": "^6.1.2",
		"ofetch": "^1.3.3",
		"set-cookie-parser-es": "^1.0.5"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.2.2",
		"@nuxt/devtools": "latest",
		"@nuxt/eslint-config": "^0.2.0",
		"@nuxt/module-builder": "^0.5.1",
		"@nuxt/schema": "^3.7.3",
		"@nuxt/test-utils": "^3.7.3",
		"@types/node": "^18.17.17",
		"changelogen": "^0.5.5",
		"npm-run-all": "^4.1.5",
		"nuxt": "^3.7.3",
		"vitest": "^0.33.0"
	}
}
